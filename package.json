{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:migrate:prod": "npx prisma generate && prisma migrate deploy && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/platform-express": "^9.3.1",
    "@nestjs/platform-socket.io": "^9.4.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/websockets": "^9.4.1",
    "@prisma/client": "^4.11.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "handlebars": "^4.7.7",
    "nodemailer": "^6.9.1",
    "pg": "^8.9.0",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.0",
    "validator": "^13.7.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.3.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/nodemailer": "^6.4.7",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "prettier-plugin-prisma": "^4.10.0",
    "prisma": "^4.11.0",
    "source-map-support": "^0.5.21"
  }
}
